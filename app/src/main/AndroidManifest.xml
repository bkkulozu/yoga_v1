<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kulozubeste">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YogaFitClass">
        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true"
            ></service>
        <activity android:name="com.example.kulozubeste.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.kulozubeste.MainActivity"/>
        <activity android:name="com.example.kulozubeste.SecondActivity"/>

        <activity android:name="com.example.kulozubeste.ThirdActivity"/>
        <activity android:name="com.example.kulozubeste.ExerciseActivity"/>

        <activity android:name="com.example.kulozubeste.FavouriteActivity"/>
        <activity android:name="com.example.kulozubeste.ProfileActivity"/>

        <activity android:name="com.example.kulozubeste.CalendarActivity"/>

        <activity android:name="com.example.kulozubeste.InfoActivity"/>

        <activity android:name="com.example.kulozubeste.CountDownActivity"/>

        <activity android:name="com.example.kulozubeste.EditDataActivity"/>
        <activity android:name="com.example.kulozubeste.ListDataActivity"/>

        <activity android:name="com.example.kulozubeste.FragmentsActivity"/>
        <activity android:name="com.example.kulozubeste.MusicServiceActivity"/>

        <activity android:name="com.example.kulozubeste.VolleyActivity"/>
        <activity android:name="com.example.kulozubeste.MailActivity"/>

    </application>

</manifest>

